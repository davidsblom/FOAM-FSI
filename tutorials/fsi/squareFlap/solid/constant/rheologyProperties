FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          rheologyProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

rheology
{
    type            linearElastic;
    rho             rho [1 -3 0 0 0 0 0]  100;
    E               E   [1 -1 -2 0 0 0 0] 2.5e5;
    nu              nu  [0 0 0 0 0 0 0]   0.35;
}

