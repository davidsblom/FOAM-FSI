FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            myMovingWallVelocity;
        value           uniform (0 0 0);
    }
    symmetry-x
    {
        type            symmetryPlane;
        value           uniform (0 0 0);
    }
    symmetry-y
    {
        type            symmetryPlane;
        value           uniform (0 0 0);
    }
}


