thermoType hPsiThermo<pureMixture<constTransport<specieThermo<hConstThermo<perfectGas>>>>>;

// cp = 1036.0
// gamma = 1.4
// p = 100000.
// rho = 1.225
// cv = cp / gamma = 740.0
// Rspecific = cp - cv = 296.0, specific gas constant
// T = p / ( rho * Rspecific ) = 275.78599007170436
// R = 8314.51 N m / kmol K
// M = R / Rspecific = 28.089560810810813
// mu = 0.
// Pr = 1.

// name, nMoles, mol weight, CP, Hf, mu, Pr;
mixture         gas 1.0 28.089560810810813 1036.0 0 0 1.0;

