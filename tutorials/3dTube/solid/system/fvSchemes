FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default backward2;
}

gradSchemes
{
    default none;
    grad(U) extendedLeastSquares       0 ;
}

divSchemes
{
    default none;
    div(sigma) Gauss linear ;
}

laplacianSchemes
{
    default none;
    laplacian(DU,U) Gauss linear corrected 1;
}

snGradSchemes
{
    default corrected;
}

interpolationSchemes
{
    default linear;
}

